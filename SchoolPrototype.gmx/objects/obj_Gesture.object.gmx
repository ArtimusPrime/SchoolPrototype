                if( mys &gt; my )
                    gesture = "FlickDiagRightUp";
                else
                    gesture = "FlickDiagRightDown";
            }
            else
            {
                if( mys &gt; my )
                    gesture = "FlickDiagLeftUp";
                else
                    gesture = "FlickDiagLeftDown";
            }
        }
    }
}
