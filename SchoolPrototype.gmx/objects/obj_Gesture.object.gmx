else
    swipe_speed = 0;

if( swipe_dist &lt; 32 )
    if( swipe_time &lt; 15 )
        gesture = "Tap";
    else
        gesture = "Hold";
else
{
    if( swipe_speed &gt; 10 )
    {
        if( abs( mxs - mx ) - abs( mys - my ) &gt; 32 )    // Horizontal
        {
            if( mx &gt; mxs )
                gesture = "FlickRight";
            else
                gesture = "FlickLeft";
        }
        else if( abs( mys - my ) - abs( mxs - mx ) &gt; 32 )   // Vertical
        {
            if( mys &gt; my )
                gesture = "FlickUp";
            else
                gesture = "FlickDown";
        }
        else
